RewriteEngine On
RewriteBase /~kristoffer/bth/mom08/

RewriteRule ^([A-Z_]+)/(id-[0-9]+)$ index.php?p=$1&id=$2 [NC,L]

RewriteRule ^([A-Z_]+)$ index.php?p=$1 [NC,L]
RewriteRule ^([A-Z_]+)/([0-9A-Z]+)$ index.php?p=$1&action=$2 [NC,L]
RewriteRule ^([A-Z_]+)/([0-9A-Z]+)/(id-[0-9]+)$ index.php?p=$1&action=$2&id=$3 [NC,L]



